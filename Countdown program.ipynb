    "import time\n",
    "def countdown(t):    \n",
    "    while t:\n",
    "        t2=divmod(t,60)\n",
    "        timeformat=(t2[0],t2[1])\n",
    "        print(timeformat,end=\"\\r\")\n",
    "        time.sleep(1)\n",
    "        t=t-1\n",
    "    print('fire in the hole!')\n",
    "t = input(\"enter time in seconds: \")\n",
    "countdown(int(t))"
   ]
  }
